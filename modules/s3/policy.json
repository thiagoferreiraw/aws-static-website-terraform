{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "OnlyCloudfrontReadAccess",
            "Principal": "*",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": "arn:aws:s3:::${domain_name}/*"
        }
    ]
}